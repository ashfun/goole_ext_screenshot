{
  "name": "Screenshots with target elements",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "Screenshots with target elements",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "downloads",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "js": ["jquery-3.4.1.min.js", "content_script.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}
